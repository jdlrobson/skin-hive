{
	"name": "Hive",
	"author": "The Hive",
	"manifest_version": 2,
	"descriptionmsg": "hive-desc",
	"namemsg": "skinname-hive",
	"type": "skin",
	"ValidSkinNames": {
		"hive": {
			"class": "SkinTemplate",
			"args": [
				{
					"name": "hive",
					"responsive": true,
					"template": "HiveTemplate",
					"scripts": [
						"skins.hive.js"
					],
					"styles": [
						"mediawiki.skinning.interface",
						"skins.hive"
					]
				}
			]
		}
	},
	"license-name": "gpl-3.0",
	"AutoloadClasses": {
		"HiveTemplate": "HiveTemplate.php"
	},
	"MessagesDirs": {
		"Hive": "i18n"
	},
	"ResourceModules": {
		"skins.hive": {
			"targets": [ "desktop", "mobile" ],
			"styles": [
				"resources/bootstrap-cyborg-4.5.3.min.css",
				"resources/bootstrap-colorfix.css",
				"resources/default.css"
			]
		},
		"skins.hive.js": {
			"targets": [ "desktop", "mobile" ],
			"scripts": [
				"resources/bootstrap-4.5.3.bundle.min.js"
			]
		}
	},
	"ResourceFileModulePaths": {
		"localBasePath": "",
		"remoteSkinPath": "Hive"
	}
}
